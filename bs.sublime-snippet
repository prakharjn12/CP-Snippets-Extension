  <content><![CDATA[
 ll l=1,r=n;
  ll iter=0,ans=1;
  while(l<=r)
  {
      assert(iter<1000);iter++;
      ll mid=(l+r+1)/2;
      if(check(mid)) {ans=mid;l=mid+1;}
      else r=mid-1;
  }
  cout<<ans<<endl;
]]></content>
