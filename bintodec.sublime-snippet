ll bintodec(string s)
{
  ll ans=0;
  ll p=1;
  ll sz=(ll)s.size();
  for(ll i=sz-1;i>=0;i--)
  {
    ans+=p*(s[i]-'0');
    p*=2;
  }
  return ans;
}
]]></content>
  <tabTrigger>bintodec</tabTrigger>
  <description>bintodec</description>
